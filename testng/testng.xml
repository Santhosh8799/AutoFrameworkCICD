<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<!--This the syntax to write comments in xml file//If we want Execute only specific Testcase we can use groups keyword to trigger -->
<listeners>
<listener class-name="Cognizant.TestComponent.Listeners"/>
</listeners>
  <test thread-count="5" name="SubmitOrder">
    <classes>
      <class name="Cognizant.Test.FrameWorkBuildTest"/>
      </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="ErrorValidation">
    <classes>
      <class name="Cognizant.Test.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
